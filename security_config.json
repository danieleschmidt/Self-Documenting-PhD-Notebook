{
  "security": {
    "enable_safe_mode": true,
    "disable_eval_exec": true,
    "require_input_validation": true,
    "enable_audit_logging": true,
    "secret_management": {
      "use_env_vars": true,
      "encrypt_at_rest": true,
      "rotate_keys": true
    },
    "access_control": {
      "enable_rbac": true,
      "require_2fa": true,
      "session_timeout": 3600
    },
    "data_protection": {
      "encrypt_pii": true,
      "anonymize_exports": true,
      "gdpr_compliance": true
    }
  }
}